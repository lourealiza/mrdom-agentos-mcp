{
  "dashboard": {
    "id": null,
    "title": "MrDom SDR User Metrics Dashboard",
    "tags": ["mrdom", "user", "engagement", "analytics"],
    "style": "dark",
    "timezone": "browser",
    "refresh": "30s",
    "time": {
      "from": "now-7d",
      "to": "now"
    },
    "panels": [
      {
        "id": 1,
        "title": "Active Users (7 days)",
        "type": "stat",
        "targets": [
          {
            "expr": "sum(mrdom_active_users_total)",
            "legendFormat": "Active Users"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "palette-classic"
            },
            "custom": {
              "hideFrom": {
                "legend": false,
                "tooltip": false,
                "vis": false
              }
            },
            "mappings": [],
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {
                  "color": "red",
                  "value": null
                },
                {
                  "color": "yellow",
                  "value": 100
                },
                {
                  "color": "green",
                  "value": 500
                }
              ]
            }
          }
        },
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 0
        }
      },
      {
        "id": 2,
        "title": "New Users (7 days)",
        "type": "stat",
        "targets": [
          {
            "expr": "sum(increase(mrdom_new_users_total[7d]))",
            "legendFormat": "New Users"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "palette-classic"
            },
            "custom": {
              "hideFrom": {
                "legend": false,
                "tooltip": false,
                "vis": false
              }
            },
            "mappings": [],
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {
                  "color": "red",
                  "value": null
                },
                {
                  "color": "yellow",
                  "value": 50
                },
                {
                  "color": "green",
                  "value": 200
                }
              ]
            }
          }
        },
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 0
        }
      },
      {
        "id": 3,
        "title": "User Engagement Score",
        "type": "stat",
        "targets": [
          {
            "expr": "avg(mrdom_user_engagement_score)",
            "legendFormat": "Engagement Score"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "palette-classic"
            },
            "custom": {
              "hideFrom": {
                "legend": false,
                "tooltip": false,
                "vis": false
              }
            },
            "mappings": [],
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {
                  "color": "red",
                  "value": null
                },
                {
                  "color": "yellow",
                  "value": 3
                },
                {
                  "color": "green",
                  "value": 4
                }
              ]
            }
          }
        },
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 8
        }
      },
      {
        "id": 4,
        "title": "User Retention Rate",
        "type": "stat",
        "targets": [
          {
            "expr": "mrdom_user_retention_rate",
            "legendFormat": "Retention Rate %"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "palette-classic"
            },
            "custom": {
              "hideFrom": {
                "legend": false,
                "tooltip": false,
                "vis": false
              }
            },
            "mappings": [],
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {
                  "color": "red",
                  "value": null
                },
                {
                  "color": "yellow",
                  "value": 60
                },
                {
                  "color": "green",
                  "value": 80
                }
              ]
            }
          }
        },
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 8
        }
      },
      {
        "id": 5,
        "title": "User Activity Timeline",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(mrdom_user_actions_total[5m])",
            "legendFormat": "User Actions/sec"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 24,
          "x": 0,
          "y": 16
        }
      },
      {
        "id": 6,
        "title": "User Demographics",
        "type": "piechart",
        "targets": [
          {
            "expr": "sum by (age_group) (mrdom_users_by_age)",
            "legendFormat": "{{age_group}}"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 24
        }
      },
      {
        "id": 7,
        "title": "User Geographic Distribution",
        "type": "piechart",
        "targets": [
          {
            "expr": "sum by (country) (mrdom_users_by_country)",
            "legendFormat": "{{country}}"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 24
        }
      },
      {
        "id": 8,
        "title": "User Device Types",
        "type": "piechart",
        "targets": [
          {
            "expr": "sum by (device_type) (mrdom_users_by_device)",
            "legendFormat": "{{device_type}}"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 32
        }
      },
      {
        "id": 9,
        "title": "User Browser Types",
        "type": "piechart",
        "targets": [
          {
            "expr": "sum by (browser) (mrdom_users_by_browser)",
            "legendFormat": "{{browser}}"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 32
        }
      },
      {
        "id": 10,
        "title": "User Session Duration",
        "type": "graph",
        "targets": [
          {
            "expr": "histogram_quantile(0.50, rate(mrdom_user_session_duration_bucket[5m]))",
            "legendFormat": "50th percentile"
          },
          {
            "expr": "histogram_quantile(0.95, rate(mrdom_user_session_duration_bucket[5m]))",
            "legendFormat": "95th percentile"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 24,
          "x": 0,
          "y": 40
        }
      },
      {
        "id": 11,
        "title": "User Feature Usage",
        "type": "table",
        "targets": [
          {
            "expr": "sum by (feature) (increase(mrdom_feature_usage_total[7d]))",
            "legendFormat": "{{feature}}"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 24,
          "x": 0,
          "y": 48
        }
      },
      {
        "id": 12,
        "title": "User Satisfaction Score",
        "type": "stat",
        "targets": [
          {
            "expr": "avg(mrdom_user_satisfaction_score)",
            "legendFormat": "Satisfaction Score"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "palette-classic"
            },
            "custom": {
              "hideFrom": {
                "legend": false,
                "tooltip": false,
                "vis": false
              }
            },
            "mappings": [],
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {
                  "color": "red",
                  "value": null
                },
                {
                  "color": "yellow",
                  "value": 3
                },
                {
                  "color": "green",
                  "value": 4
                }
              ]
            }
          }
        },
        "gridPos": {
          "h": 8,
          "w": 24,
          "x": 0,
          "y": 56
        }
      }
    ]
  }
}
