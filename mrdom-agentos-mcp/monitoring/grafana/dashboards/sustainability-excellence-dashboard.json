{
  "dashboard": {
    "id": null,
    "title": "MrDom SDR Sustainability Excellence Dashboard",
    "tags": ["mrdom", "sustainability", "excellence", "environment"],
    "style": "dark",
    "timezone": "browser",
    "refresh": "30s",
    "time": {
      "from": "now-30d",
      "to": "now"
    },
    "panels": [
      {
        "id": 1,
        "title": "Sustainability Excellence Score",
        "type": "stat",
        "targets": [
          {
            "expr": "mrdom_sustainability_excellence_score",
            "legendFormat": "Sustainability Excellence Score"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "palette-classic"
            },
            "custom": {
              "hideFrom": {
                "legend": false,
                "tooltip": false,
                "vis": false
              }
            },
            "mappings": [],
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {
                  "color": "red",
                  "value": null
                },
                {
                  "color": "yellow",
                  "value": 70
                },
                {
                  "color": "green",
                  "value": 90
                }
              ]
            }
          }
        },
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 0
        }
      },
      {
        "id": 2,
        "title": "Environmental Excellence Score",
        "type": "stat",
        "targets": [
          {
            "expr": "mrdom_environmental_excellence_score",
            "legendFormat": "Environmental Excellence Score"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "palette-classic"
            },
            "custom": {
              "hideFrom": {
                "legend": false,
                "tooltip": false,
                "vis": false
              }
            },
            "mappings": [],
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {
                  "color": "red",
                  "value": null
                },
                {
                  "color": "yellow",
                  "value": 70
                },
                {
                  "color": "green",
                  "value": 90
                }
              ]
            }
          }
        },
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 0
        }
      },
      {
        "id": 3,
        "title": "Sustainability Excellence Timeline",
        "type": "graph",
        "targets": [
          {
            "expr": "mrdom_sustainability_excellence_score",
            "legendFormat": "Sustainability Excellence"
          },
          {
            "expr": "mrdom_environmental_excellence_score",
            "legendFormat": "Environmental Excellence"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 24,
          "x": 0,
          "y": 8
        }
      },
      {
        "id": 4,
        "title": "Sustainability Excellence Dimensions Distribution",
        "type": "piechart",
        "targets": [
          {
            "expr": "sum by (dimension) (mrdom_sustainability_excellence_dimensions_distribution)",
            "legendFormat": "{{dimension}}"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 16
        }
      },
      {
        "id": 5,
        "title": "Sustainability Excellence Initiatives",
        "type": "table",
        "targets": [
          {
            "expr": "mrdom_sustainability_excellence_initiatives",
            "legendFormat": "{{initiative}}"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 16
        }
      },
      {
        "id": 6,
        "title": "Sustainability Excellence Impact",
        "type": "graph",
        "targets": [
          {
            "expr": "mrdom_sustainability_excellence_impact",
            "legendFormat": "{{impact}}"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 24,
          "x": 0,
          "y": 24
        }
      },
      {
        "id": 7,
        "title": "Overall Sustainability Excellence Score",
        "type": "stat",
        "targets": [
          {
            "expr": "mrdom_overall_sustainability_excellence_score",
            "legendFormat": "Overall Sustainability Excellence Score"
          }
        ],
        "fieldConfig": {
