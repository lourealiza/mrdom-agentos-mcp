{
  "dashboard": {
    "id": null,
    "title": "MrDom SDR Social Responsibility Dashboard",
    "tags": ["mrdom", "social", "responsibility", "impact"],
    "style": "dark",
    "timezone": "browser",
    "refresh": "30s",
    "time": {
      "from": "now-30d",
      "to": "now"
