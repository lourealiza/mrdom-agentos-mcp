{
  "dashboard": {
    "id": null,
    "title": "MrDom SDR Integration Metrics Dashboard",
    "tags": ["mrdom", "integration", "api", "webhooks"],
    "style": "dark",
    "timezone": "browser",
    "refresh": "30s",
    "time": {
      "from": "now-1h",
      "to": "now"
    },
    "panels": [
      {
        "id": 1,
        "title": "Chatwoot Integration Status",
        "type": "stat",
        "targets": [
          {
            "expr": "mrdom_chatwoot_integration_status",
            "legendFormat": "Chatwoot Status"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "palette-classic"
            },
            "custom": {
              "hideFrom": {
                "legend": false,
                "tooltip": false,
                "vis": false
              }
            },
            "mappings": [],
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {
                  "color": "red",
                  "value": null
                },
                {
                  "color": "green",
                  "value": 1
                }
              ]
            }
          }
        },
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 0
        }
      },
      {
        "id": 2,
        "title": "N8N Integration Status",
        "type": "stat",
        "targets": [
          {
            "expr": "mrdom_n8n_integration_status",
            "legendFormat": "N8N Status"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "palette-classic"
            },
            "custom": {
              "hideFrom": {
                "legend": false,
                "tooltip": false,
                "vis": false
              }
            },
            "mappings": [],
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {
                  "color": "red",
                  "value": null
                },
                {
                  "color": "green",
                  "value": 1
                }
              ]
            }
          }
        },
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 0
        }
      },
      {
        "id": 3,
        "title": "AWS Bedrock Integration Status",
        "type": "stat",
        "targets": [
          {
            "expr": "mrdom_bedrock_integration_status",
            "legendFormat": "Bedrock Status"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "palette-classic"
            },
            "custom": {
              "hideFrom": {
                "legend": false,
                "tooltip": false,
                "vis": false
              }
            },
            "mappings": [],
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {
                  "color": "red",
                  "value": null
                },
                {
                  "color": "green",
                  "value": 1
                }
              ]
            }
          }
        },
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 8
        }
      },
      {
        "id": 4,
        "title": "OpenAI Integration Status",
        "type": "stat",
        "targets": [
          {
            "expr": "mrdom_openai_integration_status",
            "legendFormat": "OpenAI Status"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "palette-classic"
            },
            "custom": {
              "hideFrom": {
                "legend": false,
                "tooltip": false,
                "vis": false
              }
            },
            "mappings": [],
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {
                  "color": "red",
                  "value": null
                },
                {
                  "color": "green",
                  "value": 1
                }
              ]
            }
          }
        },
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 8
        }
      },
      {
        "id": 5,
        "title": "Webhook Success Rate",
        "type": "stat",
        "targets": [
          {
            "expr": "rate(mrdom_webhook_success_total[5m]) / rate(mrdom_webhook_requests_total[5m]) * 100",
            "legendFormat": "Webhook Success Rate %"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "palette-classic"
            },
            "custom": {
              "hideFrom": {
                "legend": false,
                "tooltip": false,
                "vis": false
              }
            },
            "mappings": [],
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {
                  "color": "red",
                  "value": null
                },
                {
                  "color": "yellow",
                  "value": 90
                },
                {
                  "color": "green",
                  "value": 99
                }
              ]
            }
          }
        },
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 16
        }
      },
      {
        "id": 6,
        "title": "API Response Time",
        "type": "stat",
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(mrdom_api_response_time_bucket[5m]))",
            "legendFormat": "95th percentile"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "palette-classic"
            },
            "custom": {
              "hideFrom": {
                "legend": false,
                "tooltip": false,
                "vis": false
              }
            },
            "mappings": [],
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {
                  "color": "green",
                  "value": null
                },
                {
                  "color": "yellow",
                  "value": 1
                },
                {
                  "color": "red",
                  "value": 2
                }
              ]
            }
          }
        },
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 16
        }
      },
      {
        "id": 7,
        "title": "Integration Health Timeline",
        "type": "graph",
        "targets": [
          {
            "expr": "mrdom_chatwoot_integration_status",
            "legendFormat": "Chatwoot"
          },
          {
            "expr": "mrdom_n8n_integration_status",
            "legendFormat": "N8N"
          },
          {
            "expr": "mrdom_bedrock_integration_status",
            "legendFormat": "Bedrock"
          },
          {
            "expr": "mrdom_openai_integration_status",
            "legendFormat": "OpenAI"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 24,
          "x": 0,
          "y": 24
        }
      },
      {
        "id": 8,
        "title": "Webhook Performance",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(mrdom_webhook_requests_total[5m])",
            "legendFormat": "Webhook Requests/sec"
          },
          {
            "expr": "rate(mrdom_webhook_success_total[5m])",
            "legendFormat": "Webhook Success/sec"
          },
          {
            "expr": "rate(mrdom_webhook_errors_total[5m])",
            "legendFormat": "Webhook Errors/sec"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 24,
          "x": 0,
          "y": 32
        }
      },
      {
        "id": 9,
        "title": "API Usage by Endpoint",
        "type": "table",
        "targets": [
          {
            "expr": "topk(10, sum by (endpoint) (rate(mrdom_api_requests_total[5m])))",
            "legendFormat": "{{endpoint}}"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 24,
          "x": 0,
          "y": 40
        }
      },
      {
        "id": 10,
        "title": "Integration Error Rate",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(mrdom_integration_errors_total[5m])",
            "legendFormat": "Integration Errors/sec"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 24,
          "x": 0,
          "y": 48
        }
      },
      {
        "id": 11,
        "title": "Data Sync Status",
        "type": "stat",
        "targets": [
          {
            "expr": "mrdom_data_sync_status",
            "legendFormat": "Data Sync Status"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "palette-classic"
            },
            "custom": {
              "hideFrom": {
                "legend": false,
                "tooltip": false,
                "vis": false
              }
            },
            "mappings": [],
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {
                  "color": "red",
                  "value": null
                },
                {
                  "color": "green",
                  "value": 1
                }
              ]
            }
          }
        },
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 56
        }
      },
      {
        "id": 12,
        "title": "Integration Score",
        "type": "stat",
        "targets": [
          {
            "expr": "mrdom_integration_score",
            "legendFormat": "Integration Score"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "palette-classic"
            },
            "custom": {
              "hideFrom": {
                "legend": false,
                "tooltip": false,
                "vis": false
              }
            },
            "mappings": [],
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {
                  "color": "red",
                  "value": null
                },
                {
                  "color": "yellow",
                  "value": 70
                },
                {
                  "color": "green",
                  "value": 90
                }
              ]
            }
          }
        },
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 56
        }
      }
    ]
  }
}
