# MrDom SDR AgentOS + Bedrock - Ambiente de Desenvolvimento

# Aplicação
APP_NAME=MrDom SDR AgentOS + Bedrock
VERSION=1.0.0
ENVIRONMENT=development
DEBUG=true
LOG_LEVEL=DEBUG

# AWS Bedrock
AWS_ACCESS_KEY_ID=your_aws_access_key_here
AWS_SECRET_ACCESS_KEY=your_aws_secret_key_here
AWS_DEFAULT_REGION=us-east-1
BEDROCK_MODEL=amazon.nova-lite-v1:0

# OpenAI (Fallback)
OPENAI_API_KEY=your_openai_api_key_here

# Chatwoot
CHATWOOT_BASE_URL=https://app.chatwoot.com
CHATWOOT_ACCESS_TOKEN=your_chatwoot_token_here
CHATWOOT_ACCOUNT_ID=your_account_id_here
CHATWOOT_HMAC_SECRET=your_hmac_secret_here

# N8N
N8N_BASE_URL=http://localhost:5678
N8N_API_KEY=your_n8n_api_key_here

# Banco de Dados
DATABASE_URL=postgresql://postgres:password@localhost:5432/mrdom_sdr_dev

# Redis
REDIS_URL=redis://localhost:6379/0

# AgentOS
AGENTOS_ENABLED=true
AGENT_MAX_TOKENS=4000
AGENT_TEMPERATURE=0.7

# Monitoramento
PROMETHEUS_ENABLED=true
GRAFANA_PASSWORD=admin

# Segurança
SECRET_KEY=your_secret_key_here
ALLOWED_HOSTS=localhost,127.0.0.1

# CORS
CORS_ORIGINS=http://localhost:3000,http://localhost:8080

# Rate Limiting
RATE_LIMIT_ENABLED=true
RATE_LIMIT_REQUESTS=100
RATE_LIMIT_WINDOW=60
